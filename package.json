{
  "name": "middleware-api",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon ./bin/www",
    "deploy": "npm run version:patch && pm2 start ./bin/ecosystem.config.js --env production",
    "migrate": "cross-env NODE_ENV=development knex migrate:latest",
    "seed": "cross-env NODE_ENV=development knex seed:run",
    "migrate:rollback": "cross-env NODE_ENV=development knex migrate:rollback",
    "add_script": "node ./migrationTool.js create",
    "revert_migration": "node ./migrationTool.js revert",
    "list_migration": "cross-env NODE_ENV=development knex migrate:list",
    "roll_back": "cross-env NODE_ENV=development knex migrate:rollback --all",
    "version:patch": "cross-env VERSION_INCREMENT=patch node -e \"require('./utils/version.utils').autoIncrement()\"",
    "version:minor": "cross-env VERSION_INCREMENT=minor node -e \"require('./utils/version.utils').autoIncrement()\"",
    "version:major": "cross-env VERSION_INCREMENT=major node -e \"require('./utils/version.utils').autoIncrement()\"",
    "version:info": "node version-cli.js info",
    "version:current": "node version-cli.js current",
    "version:build": "node version-cli.js build",
    "version:auto": "node version-cli.js auto",
    "version:check": "node version-cli.js check-git",
    "version:tag-patch": "node version-cli.js commit-and-tag patch",
    "version:tag-minor": "node version-cli.js commit-and-tag minor",
    "version:tag-major": "node version-cli.js commit-and-tag major",
    "version:remind": "node -e \"require('./utils/version-reminder.utils').displayReminder()\"",
    "version:interactive": "node -e \"require('./utils/version-reminder.utils').interactiveUpdate()\"",
    "setup-hooks": "node -e \"console.log('Setting up git hooks...'); require('fs').copyFileSync('.git/hooks/pre-push.bat', '.git/hooks/pre-push'); console.log('âœ… Git hooks configured!')\"",
    "pre-push": "npm run version:check",
    "prestart": "npm run version:info",
    "predeploy": "npm run version:minor",
    "postinstall": "npm run setup-hooks 2>/dev/null || echo 'Git hooks setup skipped'"
  },
  "dependencies": {
    "axios": "^1.7.5",
    "bcrypt": "^6.0.0",
    "child_process": "^1.0.2",
    "commander": "^13.1.0",
    "compression": "^1.7.4",
    "console-table-printer": "^2.11.1",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "cron": "^3.1.7",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.7",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "dotenv-flow": "^3.2.0",
    "express": "^4.19.2",
    "express-rate-limit": "^8.1.0",
    "fs-extra": "^10.1.0",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "make-dir": "^5.0.0",
    "mime": "^3.0.0",
    "morgan": "~1.10.1",
    "multer": "2.0.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.14.4",
    "nodemon": "^3.1.4",
    "openai": "^6.3.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.11.3",
    "tedious": "^18.6.1",
    "uuid": "^11.1.0",
    "zod": "^4.1.12"
  }
}
